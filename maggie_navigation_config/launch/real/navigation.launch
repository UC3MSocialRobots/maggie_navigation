<?xml version="1.0" encoding="UTF-8"?>

<launch>
    <arg name="robot" />

    <!--- Run AMCL -->
    <include file="$(find maggie_navigation_config)/launch/amcl.launch">
        <arg name="robot" value="$(arg robot)" />
    </include>

    <!--- Movement Planner -->
    <include file="$(find maggie_navigation_config)/launch/move_base.launch">
        <arg name="robot" value="$(arg robot)" />
    </include>
</launch>
